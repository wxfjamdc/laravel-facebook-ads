{
  "name": "spotonlive/laravel-facebook-ads",
  "type": "library",
  "description": "Facebook integration for Laravel",
  "keywords": [
    "laravel",
    "ads",
    "facebook"
  ],
  "homepage": "https://github.com/spotonlive/laravel-facebook-ads",
  "license": "MIT",
  "authors": [
    {
      "name": "Nikolaj Petersen",
      "email": "nikolaj.lovenhardt@gmail.com",
      "homepage": "http://github.com/nikolajlovenhardt"
    }
  ],
  "require": {
    "php": ">=5.5.0",
    "laravel/framework": "5.*",
    "facebook/php-ads-sdk": "^6.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.0",
    "codeclimate/php-test-reporter": "dev-master",
    "phpmd/phpmd" : "1.4.*",
    "mockery/mockery": "~0.9.4"
  },
  "autoload": {
    "psr-4": {
      "LaravelFacebookAds\\": "src/LaravelFacebookAds"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "LaravelFacebookAdsTests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "LaravelFacebookAds\\LaravelFacebookAdsProvider"
      ],
      "aliases": {
        "FacebookAds": "LaravelFacebookAds\\Facades\\FacebookAdsFacade"
      }
    }
  }
}
